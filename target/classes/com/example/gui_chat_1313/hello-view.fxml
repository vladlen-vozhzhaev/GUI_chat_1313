<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.2" fx:controller="com.example.gui_chat_1313.HelloController">
   <children>
      <TextArea editable="false" fx:id="textArea" prefHeight="375" prefWidth="300.0" />
      <HBox prefHeight="25" prefWidth="300">
         <children>
            <TextField onAction="#handlerSend" prefWidth="225" fx:id="textField"/>
            <Button onAction="#handlerSend" mnemonicParsing="false" text="Отправить" prefWidth="75"/>
         </children>
      </HBox>
   </children>
</VBox>
